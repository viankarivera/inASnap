<h4>Sign Up</h4>
<br>
<%= link_to('Sign up with Github', '/auth/Github') %>
<br>

<%= form_for @user, url: "/signup" do |f| %>
<br>
    <%= f.label :email %>
    <%= f.email_field :email %> 
    <br>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <%= f.submit "Sign Up" %>
<% end %>

